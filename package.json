{
  "name": "nextjs-starter",
  "version": "1.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://https://github.com/TonicCreative/nextjs-starter"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TonicCreative/nextjs-starter/issues"
  },
  "scripts": {
    "dev": "cross-env BUILD_ENV=development next dev",
    "start": "next start",
    "\n ######################  NPM Secondary Pipeline  ######################": "",
    "build": "next build",
    "postbuild": "next-sitemap",
    "build:ssr": "next build",
    "build:static": "next build && next export",
    "postbuild:static": "npm-run-all postcss next-sitemap",
    "ci-prod-server": "serve out &",
    "next-sitemap": "next-sitemap",
    "postcss": "postcss ./out/**/*.css --use css-declaration-sorter --replace --no-map",
    "robots-txt-generator": "node scripts/robots-txt-generator.js",
    "\n ######################  Development Tools  ######################": "",
    "generate": "sg wizard",
    "\n ######################  Automation Tools  ######################": "",
    "prepare": "husky install",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "linters": "npm-run-all -p lint sass-lint ls-lint",
    "js-lint": "eslint \"./src/**/*.js\" -c ./.eslintrc.json --quiet --ignore-pattern .gitignore",
    "sass-lint": "stylelint \"./src/**/*.scss\"",
    "ls-lint": "node_modules/.bin/ls-lint"
  },
  "dependencies": {
    "@storyblok/react": "^1.2.0",
    "@storyblok/storyblok-editable": "^1.1.0",
    "axios": "^0.27.2",
    "classnames": "2.3.1",
    "default-passive-events": "2.0.0",
    "dotenv": "^16.0.2",
    "focus-visible": "5.2.0",
    "get-scroll": "2.0.1",
    "gsap": "^3.11.1",
    "js-cookie": "^3.0.0",
    "next": "^12.2.0",
    "no-op": "1.0.3",
    "normalize.css": "^8.0.1",
    "postcss-scss": "^4.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-use": "^17.4.0",
    "redux": "^4.2.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@jam3/stats": "1.0.1",
    "@ls-lint/ls-lint": "^1.11.2",
    "@next/bundle-analyzer": "^12.2.5",
    "@next/eslint-plugin-next": "^12.2.5",
    "@svgr/webpack": "^6.3.1",
    "chalk": "^5.0.1",
    "cross-env": "^7.0.3",
    "css-declaration-sorter": "^6.3.1",
    "eslint": "^8.23.0",
    "eslint-config-next": "^12.2.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jam3": "^0.2.3",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.7",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "generate-robotstxt": "^8.0.3",
    "husky": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-pngquant": "^9.0.2",
    "lint-staged": "^13.0.3",
    "minimist": "^1.2.5",
    "next-optimized-images": "2.6.2",
    "next-sitemap": "^3.0.5",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "prettier": "2.7.1",
    "react-dev-utils": "^12.0.1",
    "redux-devtools-extension": "2.13.9",
    "responsive-loader": "^3.0.4",
    "sass": "^1.54.8",
    "sass-loader": "^13.0.2",
    "seng-generator": "^0.9.0",
    "serve": "^14.0.1",
    "sharp": "^0.31.0",
    "shell-quote": "^1.7.3",
    "stylelint": "13.13.1",
    "stylelint-performance-animation": "^1.2.2",
    "webp-loader": "0.6.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">=16.17.0",
    "npm": ">=8.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
